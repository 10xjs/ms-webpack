{
  "name": "metalsmith-webpack",
  "version": "0.1.0",
  "description": "A webpack plugin for Metalsmith",
  "engines": {
    "node": ">=0.11"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint lib",
    "build": "./node_modules/.bin/babel lib --out-dir dist",
    "dev": "npm run build -- -w -s",
    "test": "./node_modules/.bin/mocha --harmony",
    "coverage": "node --harmony node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "node --harmony node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "supports-color": "^3.1.1",
    "vinyl-fs": "^1.0.0",
    "webpack": "^1.12.1"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.0.1",
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "co-mocha": "^1.1.2",
    "coveralls": "^2.11.4",
    "eslint": "^1.4.1",
    "istanbul": "^0.3.20",
    "jade": "^1.11.0",
    "memory-fs": "^0.2.0",
    "metalsmith": "^2.0.1",
    "metalsmith-in-place": "^1.3.1",
    "metalsmith-rename": "^1.0.0",
    "mocha": "^2.3.2",
    "mocha-lcov-reporter": "0.0.2",
    "rimraf": "^2.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nealgranger/metalsmith-webpack.git.git"
  },
  "author": "Neal Granger <neal@nealg.com> (http://nealg.com)",
  "bugs": {
    "url": "https://github.com/nealgranger/metalsmith-webpack.git/issues"
  },
  "homepage": "https://github.com/nealgranger/metalsmith-webpack.git"
}
